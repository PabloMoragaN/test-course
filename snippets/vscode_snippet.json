      "})"
    ]
  },
  "Describe Event": {
    "prefix": ["desc-event"],
    "description": "Describe event",
    "body": [
      "describe('@${1:event} | ${2:when}', () => {",
      "\t${0}",
      "})"
    ]
  },
  "Test": {
    "prefix": ["test"],
    "description": "Test skeleton",
    "body": [
      "test('${1:behaviour}', () => {",
      "\t${0}",
      "})"
    ]
  },
